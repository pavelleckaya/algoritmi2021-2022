#include <iostream>
#include <vector>

using vvl = std::vector<std::vector<long long>>;

long long Peace(int n) {
    vvl dp(n + 1, std::vector<long long>(n + 1));
    // dp[i][j] - количество способов разбиения числа i на мирные множества с максимальным возможным используемым элементом j

    for (int i = 0; i < n + 1; ++i) {
        dp[0][i] = 1;
    }
    for (int i = 1; i < n + 1; ++i) {
        for (int j = 1; j < i + 1; ++j) {
            if (i - j < j / 2) {
                dp[i][j] = dp[i][j - 1] + dp[i - j][i - j];
            }
            else {
                dp[i][j] = dp[i][j - 1] + dp[i - j][j / 2];
            }
        }
    }
    return dp[n][n];
}

int main()
{
    int n;
    std::cin >> n;
    long long ans = Peace(n);
    std::cout << ans;
}
